{
	"manifest_version": 2,
	"name": "小智助手",
	"version": "1.0",
	"description": "fast vpn.",
	"author": "hao",
	"icons": {
		"48": "/assets/icon.png",
		"128": "/assets/icon.png"
	},
	"browser_action": {
		"default_icon": "/assets/icon_pending.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": [
			"/js/crypto-js.js",
			"/js/background-script.js"
		]
	},
	"homepage_url": "https://www.baidu.com",
	"web_accessible_resources": [
		"inject.js"
	],
	"permissions": [
		"proxy", "https://*/*", "http://*/*", "background", "management", "cookies"
	]
}